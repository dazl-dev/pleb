{
  "name": "lerna-publisher",
  "description": "Utility to publish lerna/yarn/workspace types of packages from ci to npm",
  "version": "1.1.1",
  "main": "cjs/index.js",
  "bin": {
    "lerna-publisher": "cjs/cli.js"
  },
  "scripts": {
    "clean": "rimraf ./cjs; rimraf dist",
    "prebuild": "yarn clean",
    "build": "ts-build ./src --cjs",
    "test": "mocha -r @ts-tools/node/r \"./test/**/*.spec.ts\" --watch-extensions ts --timeout 50000",
    "prepack": "yarn build"
  },
  "dependencies": {
    "aws-sdk": "^2.501.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "get-monorepo-packages": "^1.2.0",
    "github-api": "^3.2.2",
    "glob": "^7.1.4",
    "mime": "^2.4.4",
    "pacote": "^9.5.4"
  },
  "devDependencies": {
    "@ts-tools/build": "^0.1.14",
    "@ts-tools/node": "^0.9.8",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/glob": "^7.1.1",
    "@types/mime": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "10",
    "@types/semver": "^6.0.1",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "cjs",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "git@github.com:wixplosives/lerna-publisher.git"
}
